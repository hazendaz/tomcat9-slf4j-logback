<site name="${project.name}" xmlns="http://maven.apache.org/SITE/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SITE/2.0.0 https://maven.apache.org/xsd/site-2.0.0.xsd">
    <bannerLeft href="https://tomcat.apache.org/">
        <image src="https://tomcat.apache.org/images/tomcat.png" />
    </bannerLeft>
    <bannerRight href="https://logback.qos.ch/">
        <image src="https://logback.qos.ch/images/logos/lblogo.jpg" />
    </bannerRight>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>2.0.0-M11</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
        </fluidoSkin>
    </custom>
    <body>
        <links>
            <item name="Tomcat Slf4j Logback" href="https://github.com/tomcat-slf4j-logback/tomca9-slf4j-logback" />
        </links>
        <menu ref="reports" />
    </body>
</site>
